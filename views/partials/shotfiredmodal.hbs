<div class="modal" tabindex="-1" id="shotsFiredModal" role="dialog">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
        <div class="modal-header">
            <h5 class="modal-title">Nytt skott</h5>
            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">&times;</span>
            </button>
        </div>
        <div class="modal-body">
            
            <label for="shotType">Kaliber</label>
            <input type="text" class="form-control shot-input" id="shotType" placeholder="Kaliber…" name="shotType">

            <label for="killshot">
                <input type="checkbox" name="killshot" class="shot-input">
                Dödande skott
            </label>
            <br />
            <label for="animalType">Djurtyp</label>
            <input type="text" class="form-control shot-input" id="animalType" placeholder="Vilket djur?" name="animalType">
        </div>
        <div class="modal-footer">
            <button type="button" class="btn btn-primary" id="addShot">Lägg till</button>
        </div>
        </div>
    </div>
</div>